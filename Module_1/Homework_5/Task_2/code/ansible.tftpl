[swarm_master]
%{for host in swarm_master ~}
${host["name"]}   ansible_host=${host["network_interface"][0]["nat_ip_address"]}
%{endfor ~}

[swarm_node]
%{for host in swarm_host ~}
${host["name"]}   ansible_host=${host["network_interface"][0]["nat_ip_address"]}
%{endfor ~}

[all:vars]
ansible_user=${remote_user}
